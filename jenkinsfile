pipeline{ 
    agent any 
    stages{
        stage("cloining the repository"){
            steps{
                script{
                     checkout([
                        $class: 'GitSCM',
                        branches: [[name: '*/main']],  // Update 'main' to the correct branch name
                        userRemoteConfigs: [[url: 'https://github.com/mohanpractice14242/jenkins-pipeline.git']]
                    ])
                }
            }
        }

    }
}